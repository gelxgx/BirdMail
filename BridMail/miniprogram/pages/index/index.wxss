/**index.wxss**/
@import"../../utils/animate.wxss";
@import"../../app.wxss";
/*轮播图*/
.lbbg{
  position: absolute;
  z-index: 1;
  background-color: white;
  width: 100%;
  height: 100vh;
}
.logo{
  position: relative;
  top: 8px;
  width: 260px;
  height: 100px;
}
.swiper{
  position: relative;
  top: 5vh;
  width: 70%;
  margin: 0 auto;
  height: 700rpx;
}
.swiper image{
  border-radius: 30px;
  display: table-cell;
  height: 500rpx;
  align-items: center;
  vertical-align: middle;
  
}
.swiper view{
  text-align: center;
  margin-top:  55rpx;
  font-size: 37rpx;
  font-weight: 500;
}
.btn{
  position: absolute;
  top: 1000rpx;
  left: 100rpx;
  width: 550rpx;
  height: 70rpx;
  border-radius: 15rpx;
  background-color: #ABE9B1;
}
.btn .wz{
  text-align: center;
  line-height: 70rpx;
  font-weight: 700;
  font-size: 35rpx;
}
/*主界面*/
.bg{
  width: 100%;
  height: 99.4vh;
  background-image: url(https://6765-gelx-rntj2-1302013920.tcb.qcloud.la/images/bg1.jpg?sign=28980735d1d2728f9a6a7c6d010bbb43&t=1588494648);
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
.help image{
  position: absolute;
  top: 23vh;
  left: 15rpx;
  width: 100rpx;
  height: 110rpx;
}
button[plain]{
  border: 0;
}
.write{
  width: 140rpx;
  height: 135rpx;
  position: absolute;
  top: 37vh;
}
.writeIcon{
  width: 113rpx;
  height: 120rpx;
  position: absolute;
  left: 7rpx;
}
.Email{
  width: 135rpx;
  height: 115rpx;
  position: absolute;
  top: 51vh;
}
.EmailIcon{
  width: 110rpx;
  height: 115rpx;
  position: absolute;
  left: 7rpx;
}
.post{
  width: 210rpx;
  height: 220rpx;
  position: absolute;
  top: 76vh;
}
.postIcon{
  width: 180rpx;
  height: 220rpx;
  position: absolute;
  left: 5rpx;
}
.NewEmail{
  width: 90rpx;
  height: 70rpx;
  position: absolute;
  top: 72vh;
  left: 160rpx;
}

/*蒙版*/
.van-popup{
  background-color:var(--popup-background-color) !important;
}
.mb{
  width: 80%;
  margin: 0 auto;
  height: 80vh;
  margin: 0 auto;
}
.mb image{
  margin: 0 auto;
  height: 80vh;
}
.btn1{
  position: absolute;
  top: 41vh;
  left: 230rpx;
  width: 300rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 18px;
  border: 1px solid rgb(224,189,62);
  font-family: "YTSJ";
  border-radius: 30px;
  box-shadow: 10px 10px 5px #BA9823 ;
}
.dot{
  position: relative;
  top: -40vh;
  left: 430rpx;
}
.btn2{
  position: absolute;
  top: 55vh;
  left: 230rpx;
  width: 300rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 18px;
  border: 1px solid rgb(182,146,31);
  font-family: "YTSJ";
  box-shadow: 13px 13px 5px #A78414 ;
  border-radius: 30px;
}
.cancel{
  display: flex;
  align-items: center;
  justify-content: center;
}
.cancel image{
  width: 120rpx;
  height: 120rpx;
}
/*字体*/
@font-face {
  font-family: "YTSJ";
   src:url(data:application/x-font-ttf;charset=utf-8;base64,)
}